{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "cli": {
        "packageManager": "npm"
    },
    "newProjectRoot": "projects",
    "projects": {
        "installer-template": {
            "projectType": "application",
            "schematics": {},
            "root": "projects/installer-template",
            "sourceRoot": "projects/installer-template/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular/build:application",
                    "options": {
                        "browser": "projects/installer-template/src/main.ts",
                        "tsConfig": "projects/installer-template/tsconfig.app.json",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "projects/installer-template/public"
                            }
                        ],
                        "styles": ["projects/installer-template/src/styles.css"]
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "1MB",
                                    "maximumError": "2MB"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "8kB",
                                    "maximumError": "16kB"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular/build:dev-server",
                    "configurations": {
                        "production": {
                            "buildTarget": "installer-template:build:production"
                        },
                        "development": {
                            "buildTarget": "installer-template:build:development"
                        }
                    },
                    "defaultConfiguration": "development",
                    "options": {
                        "port": 4201
                    }
                },
                "test": {
                    "builder": "@angular/build:unit-test"
                }
            }
        },
        "installer-builder": {
            "projectType": "application",
            "schematics": {},
            "root": "projects/installer-builder",
            "sourceRoot": "projects/installer-builder/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular/build:application",
                    "options": {
                        "browser": "projects/installer-builder/src/main.ts",
                        "tsConfig": "projects/installer-builder/tsconfig.app.json",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "projects/installer-builder/public"
                            }
                        ],
                        "styles": [
                            "projects/installer-builder/src/styles.css"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "1MB",
                                    "maximumError": "2MB"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "8kB",
                                    "maximumError": "16kB"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular/build:dev-server",
                    "configurations": {
                        "production": {
                            "buildTarget": "installer-builder:build:production"
                        },
                        "development": {
                            "buildTarget": "installer-builder:build:development"
                        }
                    },
                    "defaultConfiguration": "development",
                    "options": {
                        "port": 4200
                    }
                },
                "test": {
                    "builder": "@angular/build:unit-test"
                }
            }
        },
        "data-access": {
          "projectType": "library",
          "root": "libs/angular/data-access",
          "sourceRoot": "libs/angular/data-access/src",
          "prefix": "lib",
          "architect": {
            "build": {
              "builder": "@angular/build:ng-packagr",
              "configurations": {
                "production": {
                  "tsConfig": "libs/angular/data-access/tsconfig.lib.prod.json"
                },
                "development": {
                  "tsConfig": "libs/angular/data-access/tsconfig.lib.json"
                }
              },
              "defaultConfiguration": "production"
            },
            "test": {
              "builder": "@angular/build:unit-test",
              "options": {
                "tsConfig": "libs/angular/data-access/tsconfig.spec.json"
              }
            }
          }
        },
        "service": {
          "projectType": "library",
          "root": "libs/angular/service",
          "sourceRoot": "libs/angular/service/src",
          "prefix": "lib",
          "architect": {
            "build": {
              "builder": "@angular/build:ng-packagr",
              "configurations": {
                "production": {
                  "tsConfig": "libs/angular/service/tsconfig.lib.prod.json"
                },
                "development": {
                  "tsConfig": "libs/angular/service/tsconfig.lib.json"
                }
              },
              "defaultConfiguration": "production"
            },
            "test": {
              "builder": "@angular/build:unit-test",
              "options": {
                "tsConfig": "libs/angular/service/tsconfig.spec.json"
              }
            }
          }
        },
        "tauri": {
          "projectType": "library",
          "root": "libs/angular/tauri",
          "sourceRoot": "libs/angular/tauri/src",
          "prefix": "lib",
          "architect": {
            "build": {
              "builder": "@angular/build:ng-packagr",
              "configurations": {
                "production": {
                  "tsConfig": "libs/angular/tauri/tsconfig.lib.prod.json"
                },
                "development": {
                  "tsConfig": "libs/angular/tauri/tsconfig.lib.json"
                }
              },
              "defaultConfiguration": "production"
            },
            "test": {
              "builder": "@angular/build:unit-test",
              "options": {
                "tsConfig": "libs/angular/tauri/tsconfig.spec.json"
              }
            }
          }
        },
        "api-contracts": {
          "projectType": "library",
          "root": "libs/angular/api-contracts",
          "sourceRoot": "libs/angular/api-contracts/src",
          "prefix": "lib",
          "architect": {
            "build": {
              "builder": "@angular/build:ng-packagr",
              "configurations": {
                "production": {
                  "tsConfig": "libs/angular/api-contracts/tsconfig.lib.prod.json"
                },
                "development": {
                  "tsConfig": "libs/angular/api-contracts/tsconfig.lib.json"
                }
              },
              "defaultConfiguration": "production"
            },
            "test": {
              "builder": "@angular/build:unit-test",
              "options": {
                "tsConfig": "libs/angular/api-contracts/tsconfig.spec.json"
              }
            }
          }
        }
    }
}
