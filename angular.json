{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "cli": {
        "packageManager": "npm"
    },
    "newProjectRoot": "projects",
    "projects": {
        "installer-template": {
            "projectType": "application",
            "schematics": {},
            "root": "projects/installer-template",
            "sourceRoot": "projects/installer-template/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular/build:application",
                    "options": {
                        "browser": "projects/installer-template/src/main.ts",
                        "tsConfig": "projects/installer-template/tsconfig.app.json",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "projects/installer-template/public"
                            }
                        ],
                        "styles": ["projects/installer-template/src/styles.css"]
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kB",
                                    "maximumError": "1MB"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "4kB",
                                    "maximumError": "8kB"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular/build:dev-server",
                    "configurations": {
                        "production": {
                            "buildTarget": "installer-template:build:production"
                        },
                        "development": {
                            "buildTarget": "installer-template:build:development"
                        }
                    },
                    "defaultConfiguration": "development",
                    "options": {
                        "port": 4201
                    }
                },
                "test": {
                    "builder": "@angular/build:unit-test"
                }
            }
        },
        "installer-builder": {
            "projectType": "application",
            "schematics": {},
            "root": "projects/installer-builder",
            "sourceRoot": "projects/installer-builder/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular/build:application",
                    "options": {
                        "browser": "projects/installer-builder/src/main.ts",
                        "tsConfig": "projects/installer-builder/tsconfig.app.json",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "projects/installer-builder/public"
                            }
                        ],
                        "styles": ["projects/installer-builder/src/styles.css"]
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kB",
                                    "maximumError": "1MB"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "4kB",
                                    "maximumError": "8kB"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular/build:dev-server",
                    "configurations": {
                        "production": {
                            "buildTarget": "installer-builder:build:production"
                        },
                        "development": {
                            "buildTarget": "installer-builder:build:development"
                        }
                    },
                    "defaultConfiguration": "development",
                    "options": {
                        "port": 4200
                    }
                },
                "test": {
                    "builder": "@angular/build:unit-test"
                }
            }
        },
        "installer-core": {
            "projectType": "library",
            "root": "projects/installer-core",
            "sourceRoot": "projects/installer-core/src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular/build:ng-packagr",
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/installer-core/tsconfig.lib.prod.json"
                        },
                        "development": {
                            "tsConfig": "projects/installer-core/tsconfig.lib.json"
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "test": {
                    "builder": "@angular/build:unit-test",
                    "options": {
                        "tsConfig": "projects/installer-core/tsconfig.spec.json"
                    }
                }
            }
        }
    }
}
